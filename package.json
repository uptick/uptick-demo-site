{
  "name": "uptick-demo-site",
  "version": "1.3.2",
  "description": "Content for creating Uptick branded demo sites",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "mkdir -p dist && cp src/base.html dist/base.html && node scripts/build.js",
    "watch": "mkdir -p dist && cp src/base.html dist/base.html && node scripts/watch.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uptick/uptick-demo-site.git"
  },
  "keywords": [
    "uptick",
    "demo",
    "demonstration",
    "npm",
    "package",
    "example"
  ],
  "author": "Uptick Pty Ltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uptick/uptick-demo-site/issues"
  },
  "homepage": "https://github.com/uptick/uptick-demo-site#readme",
  "devDependencies": {
    "esbuild": "^0.20.0",
    "esbuild-stylus-loader": "^0.4.2",
    "typescript": "^5.3.3"
  }
}
